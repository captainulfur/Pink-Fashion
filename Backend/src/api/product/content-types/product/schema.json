{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "default": 0
    },
    "brandName": {
      "type": "string"
    },
    "info": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "smal",
        "medom",
        "larg",
        "xlarg",
        "xxlarg",
        "xxxlarg"
      ]
    },
    "coler": {
      "type": "enumeration",
      "enum": [
        "red",
        "blue",
        "black",
        "pink",
        "yellow",
        "orange"
      ]
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "female",
        "man"
      ]
    },
    "score": {
      "type": "decimal"
    }
  }
}
